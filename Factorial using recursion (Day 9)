#include <stdio.h>
long double fact(long double);
int main()
{
    double num,f;
    printf("\n enter the number:");
    scanf("%lf",&num);
    f=fact (num);
    printf("\n factorial of the number %lf: %lf",num,f);
    return 0;
}
long double fact(long double x)
{
    if(x==1)
    {
        return 1;
    }
    else
    {
        return(x*fact(x-1));
        
    }
    }
