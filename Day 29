#include <stdio.h>
void goodmorning();
void goodafternoon();
void goodnight();

int main()
{
    goodmorning();
    goodafternoon();
    goodnight();
    return 0;
}

void goodmorning()
{
    printf("good morning saransh\n");
}
void goodafternoon()
{
    printf("good afternoon saransh\n");
}
void goodnight()
{
    printf("good night saransh\n");
}
